{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "arrow-parens": "off",
    "comma-dangle": "off",
    "object-curly-spacing": "off",
    "eol-last": "off",
    "require-jsdoc": "off",
    "valid-jsdoc": "off",
    "linebreak-style": "off",
    "keyword-spacing": "off",
    "max-len": [
      "error",
      {"code": 120}
    ]
  },
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "plugins": ["jest"]
}